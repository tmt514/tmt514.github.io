out << \"QUERY \" << x << \" \" << (x + WINDOW-1)%FULL << endl;\n  int ret;\n  cin >> ret;\n  return ret;\n}\n\nint GetMiddlePoint(int l, int r) {\n  int mid = (l > r?\n    (l + r + FULL) / 2 :\n    (l + r) / 2\n  );\n  return mid % FULL;\n}\n\nint main() {\n  int N;\n  cin >> N;\n  \n  int vl = MakeQuery(0);\n  int vr = N - vl;\n  int l = 0, r = WINDOW;\n\n  if (vl == vr) {\n    cout << \"YES \" << 0 << endl;\n    return 0;\n  }\n\n  if (vl > vr) swap(l, r);\n  \n  while (l != r) {\n    int m = GetMiddlePoint(l, r);\n    int v = MakeQuery(m);\n    if (v == N/2) {\n      cout << \"YES \" << m << endl;\n      return 0;\n    } else if (v > N/2) {\n      r = (m+FULL-1)%FULL;\n    } else {\n      l = (m+1)%FULL;\n    }\n  }\n\n  cout << \"YES \" << l << endl;\n  return 0;\n}\n"}]}]},{"type":"text","value":"\n"},{"type":"element","tagName":"h3","properties":{},"children":[{"type":"text","value":"é—œæ–¼ç«¶ç¨‹æ—¥è¨˜"}]},{"type":"text","value":"\n"},{"type":"element","tagName":"p","properties":{},"children":[{"type":"text","value":"ğŸ… å¦‚æœæ‚¨æƒ³åˆ°æ›´å¤šæœ‰è¶£æ¼‚äº®ç°¡å–®ä¹¾æ·¨çš„è§£æ³•è©±æ­¡è¿ç•™è¨€çµ¦ç«¶ç¨‹æ—¥è¨˜å°ç·¨ç¾¤ï¼"}]},{"type":"text","value":"\n"},{"type":"element","tagName":"p","properties":{},"children":[{"type":"text","value":"â„¹ï¸ é€™æ˜¯ä¸€ç¯‡æŠ•ç¨¿çµ¦"},{"type":"element","tagName":"a","properties":{"href":"https://www.facebook.com/%E7%AB%B6%E7%A8%8B%E6%97%A5%E8%A8%98-1514973425463954/"},"children":[{"type":"text","value":"ç«¶ç¨‹æ—¥è¨˜"}]},{"type":"text","value":"çš„æ–‡ç« ï¼Œæ­¡è¿å¤§å®¶æŠ•ç¨¿ã€äº¤æµèˆ‡åˆ†äº«ç¨‹å¼è§£é¡Œç«¶è³½çš„é»é»æ»´æ»´ï¼"}]}],"data":{"quirksMode":false}},"frontmatter":{"date":"January 12, 2019","path":"/problem/icpc/asia_hanoi_2018/D","title":"Divide Doughnut","description":"**é€™æ˜¯ä¸€å€‹äº’å‹•é¡Œã€‚**\n\n![](https://open.kattis.com/problems/dividedoughnut/file/statement/en/img-0001.png)\n\nKhue èˆ‡ Hanh ç²å¾—äº†ä¸€å€‹ä¸Šé¢ç‘äº†ä¸€äº›äº”å½©å·§å…‹åŠ›ç±³çš„ç”œç”œåœˆã€‚ä»–å€‘æƒ³è¦æŠŠä¸€å€‹ç”œç”œåœˆåˆ‡æˆå…¬å¹³çš„ä¸€åŠï¼Œä½¿å¾—å…©é‚Šçš„å·§å…‹